<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
 <package name="cn.edu.hezeu.jsj.login" namespace="/login" extends="struts-default">
 
   <action name="Logincheck" class="LoginAction" method="execute">
    <result name="success">/admin/center1.jsp</result>
    <result name="failure">/login.jsp</result>
    <result name="teachersuccess">/teacher/center1.jsp</result>
    <result name="studentsuccess">/student/center1.jsp</result>
   </action>
 </package>
</struts>
